{
  "name": "aws-agritrace",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "caver-js": "^0.8.3-1",
    "connected-react-router": "^6.4.0",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-js-pagination": "^3.0.2",
    "react-redux": "^7.0.3",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
